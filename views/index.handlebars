<div class="matches-played-per-year">
  <h2> Matches Played Per Season</h2>
  <table>
    {{> createTable matchesPlayedResult}}
  </table>
</div>

<div class="matches-won-per-team-per-year">
  <h2> Matches won per team per year</h2>
  <div>
    {{#each matchesWinPerTeamResult}}
    <table>
      <caption>{{@key}}</caption>
      {{> createTable this}}
    </table>
    {{/each}}
  </div>
</div>

<div class="extra-runs">
  <h2> Extra runs conceded per team in the year 2016</h2>
  <table>
    {{> createTable extraRunsResult}} </table>
</div>

<div class="bowlers-economy">
  <h2> Bowlers economy in the year 2015</h2>
  <table>
    {{> createTable economicalBowlersResult}} </table>
</div>